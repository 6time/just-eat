<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="agaig.justeat.jiwon.model.aws.dao.TestMapper">

    <select id="getAllDataList" resultType="agaig.justeat.jiwon.model.aws.dto.portfolio">
        SELECT * FROM portfolio;
    </select>

    <insert id="insertList">
        INSERT INTO PORTFOLIO(start_data,deadline,header,description,image_path)
        values(#{start_data},#{deadline},#{header},#{description},#{image_path});
    </insert>
</mapper>